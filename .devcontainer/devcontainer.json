{
    "dockerComposeFile": "compose.yaml",
    "service": "app",
    "workspaceFolder": "/app",
    "features": {
        "ghcr.io/devcontainers/features/git:1": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-playwright.playwright"
            ]
        }
    },
    "forwardPorts": [
        5173,
        8000,
        8025
    ],
    "shutdownAction": "none"
}